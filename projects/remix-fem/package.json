{
    "name": "remix-fem",
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "remix build",
        "dev": "run-p \"dev:*\"",
        "dev:css": "pnpm generate:css --watch",
        "dev:remix": "remix dev",
        "build:css": "pnpm generate:css --minify",
        "start": "remix-serve build",
        "generate:css": "tailwindcss -i ./styles/tailwind.css -o app/styles/tailwind.css"
    },
    "dependencies": {
        "@prisma/client": "^4.6.1"
    },
    "devDependencies": {
        "prisma": "^4.6.1"
    },
    "engines": {
        "node": ">=14"
    }
}
